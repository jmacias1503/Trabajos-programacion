#include <stdio.h>
int main() {
   float a,b,c,d;
   float r,i;
   printf("a =");
   scanf("%f",&a);
   printf("\nb =");
   scanf("%f", &b);
   printf("\nc =");
   scanf("%f", &c);
   printf("\nd =");
   scanf("%f", &d);
   r = (a*c + b*d)/(c*c + d*d);
   i = (b*c - a*d)/(c*c + d*d);
   printf("z =", r);
}
